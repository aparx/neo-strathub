{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pnpm run sb:gen:local && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0",
    "sb:gen:local": "npx supabase gen types typescript --local > ./src/utils/supabase/types.ts",
    "sb:reseed": "npx tsx seed.ts > supabase/seed.sql && npx supabase db reset",
    "sb:reset": "pnpm run sb:reseed && pnpm run sb:gen:local",
    "sb:restart": "npx supabase stop && npx supabase start",
    "sb:serve": "npx supabase functions serve"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-visually-hidden": "^1.0.3",
    "@repo/canvas": "workspace:^",
    "@repo/theme": "workspace:^",
    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:^",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.43.2",
    "@tanstack/react-query": "^5.29.2",
    "@vanilla-extract/css": "^1.14.2",
    "@vanilla-extract/css-utils": "^0.1.3",
    "@vanilla-extract/recipes": "^0.5.2",
    "framer-motion": "^11.1.7",
    "its-fine": "^1.2.5",
    "moment": "^2.30.1",
    "next": "14.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.4",
    "react-icons": "^5.0.1",
    "react-konva": "^18.2.10",
    "server-act": "^1.1.7",
    "server-only": "^0.0.1",
    "use-debounce": "^10.0.0",
    "usehooks-ts": "^3.1.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.3",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@snaplet/copycat": "^5.0.0",
    "@snaplet/seed": "^0.96.0",
    "@types/eslint": "^8.56.5",
